echo "naca_square"
./setupGeom naca_square

echo "Steady"
sed 's/lres/F/
     s/dtPrint/0.1/
     s/timeEnd/7.1/
     s/bodyRwnd/T/
     s/bodyMove/F/' temp.txt > inp.txt
./runBDIM 12 squareSteady &
wait

echo "Impulse"
sed 's/lres/T/
     s/dtPrint/0.01/
     s/timeEnd/1.00/
     s/bodyRwnd/F/
     s/bodyMove/T/' temp.txt > inp.txt
./runBDIM 12 squareImpulse squareSteady &
wait

echo "naca_half"
./setupGeom naca_half

echo "Steady"
sed 's/lres/F/
     s/dtPrint/0.1/
     s/timeEnd/7.1/
     s/bodyRwnd/T/
     s/bodyMove/F/' temp.txt > inp.txt
./runBDIM 12 halfSteady &
wait

echo "Impulse"
sed 's/lres/T/
     s/dtPrint/0.01/
     s/timeEnd/1.00/
     s/bodyRwnd/F/
     s/bodyMove/T/' temp.txt > inp.txt
./runBDIM 12 halfImpulse halfSteady &
wait

echo "naca_full"
./setupGeom naca_full

echo "Steady"
sed 's/lres/F/
     s/dtPrint/0.1/
     s/timeEnd/7.1/
     s/bodyRwnd/T/
     s/bodyMove/F/' temp.txt > inp.txt
./runBDIM 12 fullSteady &
wait

echo "Impulse"
sed 's/lres/T/
     s/dtPrint/0.01/
     s/timeEnd/1.00/
     s/bodyRwnd/F/
     s/bodyMove/T/' temp.txt > inp.txt
./runBDIM 12 fullImpulse fullSteady &
wait

echo "runGeoms:Complete"
